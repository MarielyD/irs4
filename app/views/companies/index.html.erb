<p id="notice"><%= notice %></p>

<h1>Companies</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Company Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Partners</th>
      <th>Many Partners</th>
      <th>Corporation</th>
      <th>S Corp</th>
      <th>LLC</th>
      <th>Sole Prop</th>
      <th>Employees</th>
      <th>Employee Benefits</th>
      <th>Contractors</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.user %></td>
        <td><%= company.name %></td>
        <td><%= company.email %></td>
        <td><%= company.phone %></td>
        <td><%= company.partners %></td>
        <td><%= company.manypartners %></td>
        <td><%= company.corporation %></td>
        <td><%= company.scorp %></td>
        <td><%= company.llc %></td>
        <td><%= company.soleprop %></td>
        <td><%= company.employees %></td>
        <td><%= company.employeebenefits %></td>
        <td><%= company.contractors %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
