<h1>Sessions#new</h1>
<h1>Log In</h1>
<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag 'Log in' %>
  </p>
  Don't have an account?
    <%= link_to 'Sign Up Here', users_new_path %>
<% end  %>
